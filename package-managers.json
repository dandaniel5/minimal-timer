{
  "package_name": "minimal-timer",
  "version": "1.0.0",
  "description": "A minimalist command-line timer with smart time parsing and system integration",
  "author": "Danil Kodolov",
  "homepage": "https://github.com/dandaniel5/minimal-timer",
  "license": "MIT",
  "managers": {
    "pypi": {
      "enabled": true,
      "priority": 1,
      "package_name": "minimal-timer",
      "command": "python3 -m twine upload dist/*",
      "build_command": "python3 setup.py sdist bdist_wheel",
      "platforms": [
        "Windows",
        "macOS",
        "Linux",
        "Unix"
      ],
      "status": "published"
    },
    "homebrew": {
      "enabled": true,
      "priority": 2,
      "package_name": "dandaniel5/timer/timer",
      "repository": "https://github.com/dandaniel5/homebrew-timer",
      "platforms": [
        "macOS",
        "Linux"
      ],
      "status": "published"
    },
    "snap": {
      "enabled": true,
      "priority": 3,
      "package_name": "minimal-timer",
      "command": "snapcraft upload --release=stable *.snap",
      "build_command": "snapcraft",
      "platforms": [
        "Linux"
      ],
      "status": "not_published"
    },
    "aur": {
      "enabled": true,
      "priority": 4,
      "package_name": "minimal-timer",
      "repository": "https://aur.archlinux.org/minimal-timer.git",
      "platforms": [
        "Arch Linux"
      ],
      "status": "not_published"
    },
    "winget": {
      "enabled": true,
      "priority": 5,
      "package_name": "DanilKodolov.MinimalTimer",
      "repository": "https://github.com/microsoft/winget-pkgs",
      "platforms": [
        "Windows"
      ],
      "status": "not_published"
    },
    "chocolatey": {
      "enabled": true,
      "priority": 6,
      "package_name": "minimal-timer",
      "command": "choco push minimal-timer.*.nupkg --source https://push.chocolatey.org/",
      "build_command": "choco pack",
      "platforms": [
        "Windows"
      ],
      "status": "not_published"
    },
    "scoop": {
      "enabled": true,
      "priority": 7,
      "package_name": "minimal-timer",
      "repository": "https://github.com/dandaniel5/scoop-bucket",
      "platforms": [
        "Windows"
      ],
      "status": "not_published"
    },
    "flatpak": {
      "enabled": true,
      "priority": 8,
      "package_name": "io.github.dandaniel5.MinimalTimer",
      "command": "flatpak-builder --repo=repo --force-clean build-dir io.github.dandaniel5.MinimalTimer.yml",
      "platforms": [
        "Linux"
      ],
      "status": "not_published"
    },
    "apt": {
      "enabled": true,
      "priority": 9,
      "package_name": "minimal-timer",
      "build_command": "dpkg-deb --build minimal-timer",
      "platforms": [
        "Debian",
        "Ubuntu"
      ],
      "status": "not_published"
    },
    "rpm": {
      "enabled": true,
      "priority": 10,
      "package_name": "minimal-timer",
      "build_command": "rpmbuild -ba minimal-timer.spec",
      "platforms": [
        "Fedora",
        "RHEL",
        "CentOS"
      ],
      "status": "not_published"
    },
    "nix": {
      "enabled": true,
      "priority": 11,
      "package_name": "minimal-timer",
      "repository": "https://github.com/NixOS/nixpkgs",
      "platforms": [
        "NixOS",
        "Linux",
        "macOS"
      ],
      "status": "not_published"
    },
    "npm": {
      "enabled": false,
      "priority": 12,
      "package_name": "minimal-timer-cli",
      "command": "npm publish",
      "platforms": [
        "Windows",
        "macOS",
        "Linux"
      ],
      "status": "not_published",
      "note": "Optional wrapper for Node.js users"
    },
    "freebsd": {
      "enabled": true,
      "priority": 13,
      "package_name": "minimal-timer",
      "repository": "https://www.freshports.org/",
      "platforms": [
        "FreeBSD"
      ],
      "status": "not_published"
    }
  }
}
